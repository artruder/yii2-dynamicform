{
    "name": "bajadev/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "bajadev",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/bajadev/yii2-dynamicform/issues",
        "wiki": "https://github.com/bajadev/yii2-dynamicform/wiki/",
        "source": "https://github.com/bajadev/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "Bajadev",
            "email": "info@bajadev.hu",
            "homepage": "http://bajadev.hu"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "symfony/dom-crawler": "*",
        "symfony/css-selector": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "bajadev\\dynamicform\\": "src"
        }
    }
}
